{
  "productName": "Rasyti",
  "version": "0.1.0",
  "identifier": "eu.blueseal.rasyti",
  "build": {
    "devUrl": "http://localhost:3000",
    "frontendDist": "../src-gui/dist",
    "beforeDevCommand": {
      "cwd": "../src-gui",
     "_comment": "orig script: yarn run dev",
      "script": "npm run dev"
    },
    "beforeBuildCommand": {
      "cwd": "../src-gui"
    }
  },
  "app": {
    "windows": [
      {
        "title": "Ra≈°yti",
        "width": 1024,
        "height": 1024
      }
    ],
    "security": {
      "dangerousDisableAssetCspModification": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
